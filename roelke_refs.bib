@article{Gates_ballute,
abstract = {Previous literature indicates that aerocapture with ballutes may provide significant mass advantages over traditional propulsive orbit capture. The possibility of using ballutes in the exploration of the atmosphere-bearing bodies in the solar system is considered, including Venus, Earth, Mars, Jupiter, Saturn, Titan, Uranus, and Neptune. The mass cost of ballute aerocapture is compared with propulsive capture and with previous work in which tethers are used for aerocapture in solar system exploration. Although both ballute and tether technologies require further analysis in the areas of structures, materials, and guidance, both systems offer attractive advantages over aerocapture with aeroshells and over propulsive capture. Copyright {\textcopyright} 2010 by Kristin L. Gates and James M. Longuski.},
