@article{Brunner2012,
abstract = {The dramatic increase in computational power since the Apollo program has enabled the development of numerical predictor-corrector (NPC) entry guidance algorithms that allow on-board accurate determination of a vehicle's trajectory. These algorithms are sufficiently mature to be flown. NPC algorithms are highly adaptive, especially in the face of extreme dispersion and off-nominal situations. The performance and reliability of entry guidance are critical to successfulmissions. This paper compares the performance of a recently developed NPC skip entry guidance algorithm with that of the Apollo skip entry guidance. Through extensive dispersion testing, it is clearly demonstrated that the Apollo skip entry guidance algorithm would be severely inadequate in meeting the landing precision requirement for missions with medium and long ranges under moderate dispersions. In the presence of large dispersions, a significant number of failures occur even for short-range missions. The NPC algorithm, on the other hand, is able to ensure high landing precision in all cases tested. I.},
author = {Brunner, Christopher W. and Lu, Ping},
doi = {10.1007/S40295-014-0005-1},
file = {:C$\backslash$:/Users/Evan Roelke/Documents/research/papers/ComparisonOfFullyNumericalPredictor-CorrectorandApolloSkipEntryGuidanceAlgorithms.pdf:pdf},
isbn = {4029501400},
issn = {00219142},
journal = {Journal of the Astronautical Sciences},
keywords = {3DOF simulation,Apollo entry,Dispersion testing,Entry guidance,Monte Carlo,Numerical predictor-corrector,Skip entry},
number = {3},
pages = {517--540},
title = {{Comparison of fully numerical predictor-corrector and Apollo skip entry guidance algorithms}},
volume = {59},
year = {2012}
}
@article{Bryant2013,
abstract = {The 2001 Mars mission was to demonstrate precision landing and aerocapture which may be required for later sample returns and human missions with pre-deployed habitats and low acceleration constraints. A typical entry trajectory will perform a direct Mars atmospheric phase followed by a parachute deploy and then powered descent, while an aerocapture trajectory performs an atmospheric entry and then an exit phase targeting to apoapsis altitude and a specific plane. An analytic drag control algorithm, which does not require numeric integration, has been developed for atmospheric entry and aerocapture in order to minimize the footprint at chute deploy, and minimize the delta velocity required to get to a circular orbit in the right plane. Mission requirements and constraints are reviewed followed by a discussion of the development and application of analytic drag control and the similarities between entry and aerocapture. Results from a six degree of freedom simulation will also be presented. Preliminary findings show that footprint requirements can be met with an analytic approach, and delta velocities within limits for the aerocapture,},
author = {Bryant, L. and Tigges, M. and Ives, D.},
doi = {10.2514/6.1998-4572},
file = {:C$\backslash$:/Users/Evan Roelke/Documents/research/papers/6.1998-4572.pdf:pdf},
title = {{Analytic drag control for precision landing and aerocapture}},
year = {2013}
}
